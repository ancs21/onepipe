{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["@types/bun"],
    "baseUrl": "../..",
    "paths": {
      "@onepipe/sdk": ["packages/sdk/src/index.ts"],
      "@onepipe/sdk/*": ["packages/sdk/src/*"]
    }
  },
  "include": ["./*.ts"],
  "exclude": ["../../packages"]
}
