{
  "name": "@onepipe/client",
  "version": "0.3.0",
  "description": "Type-safe client SDK for OnePipe with React hooks",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "keywords": [
    "onepipe",
    "client",
    "react",
    "hooks",
    "typescript",
    "type-safe"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ancs21/onepipe.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/ancs21/onepipe#readme",
  "bugs": "https://github.com/ancs21/onepipe/issues",
  "license": "MIT",
  "files": [
    "src",
    "!src/**/*.test.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/index.ts",
    "./react": "./src/react/index.ts"
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@onepipe/sdk": "^0.3.0",
    "@tanstack/react-query": "^5.62.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "better-auth": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "better-auth": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "typescript": "^5.0.0"
  }
}
